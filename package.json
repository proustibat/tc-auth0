{
    "name": "@samhwang/vite-react-starter-template",
    "version": "1.0.0",
    "description": "Vite + React Starter Template",
    "msw": {
        "workerDirectory": [
            "public"
        ]
    },
    "type": "module",
    "packageManager": "pnpm@10.5.2",
    "scripts": {
        "prepare": "pnpm run githooks:init",
        "githooks:init": "husky",
        "msw:init": "msw init public --save",
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "typecheck": "tsc",
        "test": "vitest",
        "test:run": "vitest run",
        "test:watch": "vitest watch",
        "test:CI": "pnpm run test && pnpm run typecheck",
        "test:preview": "vitest-preview",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "lint:fix:unsafe": "biome check --write --unsafe .",
        "ci": "biome ci ."
    },
    "dependencies": {
        "@apollo/client": "^3.13.8",
        "@auth0/auth0-react": "^2.3.0",
        "@tailwindcss/vite": "^4.1.7",
        "@tanstack/react-form": "^1.0.0",
        "@tanstack/react-query": "^5.67.1",
        "@tanstack/react-query-devtools": "^5.67.1",
        "@tanstack/react-router": "^1.120.7",
        "@tanstack/react-router-devtools": "^1.120.7",
        "@tanstack/zod-form-adapter": "^0.42.1",
        "graphql": "^16.11.0",
        "jwt-decode": "^4.0.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@headlessui/react": "^2.2.4",
        "@heroicons/react": "^2.2.0",
        "@tanstack/router-cli": "^1.112.12",
        "@tanstack/router-vite-plugin": "^1.112.12",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/node": "^22.13.9",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-v8": "^3.0.7",
        "@vitest/ui": "^3.0.7",
        "autoprefixer": "^10.4.21",
        "classnames": "^2.5.1",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^9.1.7",
        "jsdom": "^26.0.0",
        "lint-prepush": "^3.0.2",
        "lint-staged": "^15.4.3",
        "msw": "^2.7.3",
        "postcss": "^8.5.3",
        "tailwindcss": "^4.1.7",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "vite": "^6.2.0",
        "vitest": "^3.0.7",
        "vitest-preview": "^0.0.1"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "msw"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
